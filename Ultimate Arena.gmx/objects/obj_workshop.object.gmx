{
    if(map_id == steamDetails[i]) &amp;&amp; (result == ugc_result_success)
    {
        if(async_load[? "tags"] != "Character")
            continue;
        with(workshopWindow)
        {
            with(obj_uiWorkshopItem)
            {
                if(workshopID == string(global.createdWorkshopItems[i]))
                    continue;
            }
        
            with(zui_create(0,other.itemCount*120+24,obj_uiWorkshopItem))
            {
                zui_set_size(300,120);
                zui_set_anchor(0,0);
                name = async_load[? "title"];
                description = async_load[? "description"];
                visibility = async_load[? "visibility"];
                tags = async_load[? "tags"];
                votes = async_load[? "votes_up"];
                workshopID = string(global.createdWorkshopItems[i]);
                dateCreated = date_date_string(date_timestamp(async_load[? "time_created"]));
                dateUpdated = date_date_string(date_timestamp(async_load[? "time_updated"]));
            }
        }
        confirmedItems[i] = 1;
        itemCount++;
        
        with(obj_uiWindowCaption)
            if(type == 99)
                caption = "Your Workshop Items (" + string(other.itemCount) + ")";
    }
