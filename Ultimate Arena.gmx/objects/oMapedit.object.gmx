        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Map Editing

if(mouse_check_button(mb_left)){
if(mouse_x &gt; round(window_get_width()*.5-257) &amp;&amp; mouse_x &lt; round(window_get_width()*.5+256)){
if(mouse_y &gt; round(window_get_height()*.5-233) &amp;&amp; mouse_y &lt; round(window_get_height()*.5+280)){
    var xx = round((mouse_x-round(window_get_width()*.5-255))/2);
    var yy = round((mouse_y-round(window_get_height()*.5-231))/2);
    var tx = mouse_x-(round(window_get_width()*.5-256));
    var ty = mouse_y-(round(window_get_height()*.5-232));
    show_debug_message(string(xx)+','+string(yy)+','+string(tx)+','+string(ty));
    surface_set_target(mapeditSurf);
    switch(cursorType){
        case 0:
            draw_circle_center(xx,yy,cursorSize,blue);
            break;
        case 1:
            draw_circle_center(xx,yy,cursorSize,yellow);
            break;
        case 2:
            draw_circle_center(xx,yy,cursorSize,green);
            break;
        case 3:
            draw_circle_center(xx,yy,cursorSize,brown1);
            break;
        case 4:
            draw_circle_center(xx,yy,cursorSize,brown2);
            break;
        case 5:
            draw_circle_center(xx,yy,cursorSize,brown3);
            break;
        case 6:
            if(mouse_check_button_pressed(mb_left)){
                if(array_height_2d(treeArray) &lt; 30){
                    treeArray[array_height_2d(treeArray),0] = tx;
                    treeArray[array_height_2d(treeArray)-1,1] = ty;
                }
                with(objUILabel){
                    if(lID == 1)
                        caption = string(array_height_2d(oMapedit.treeArray))+"/30 Trees";
                }
            }
            break;
    }
    if(cursorType &lt; 6){
        ds_grid_set_disk(mapGrid,xx,yy,cursorSize,cursorType);
        show_debug_message(string(cursorSize)+','+string(ds_grid_get(mapGrid,xx,yy)));
    }
    surface_reset_target();
}
}
}
