            <string>draw_set_halign(fa_left);
draw_set_valign(fa_top);

//drawing the stroke
if(active)
    draw_set_colour(acol1); //active
else
    draw_set_colour(pcol1); //passive

for(var i=0; i&lt;lwidth; i++)
    draw_rectangle(round(x-i),round(y-i),round(x+i+width),round(y+i+height),1);
    
//drawing the fill
if(active)
    draw_set_colour(acol2); //active
else
    draw_set_colour(pcol2); //passive
   
draw_rectangle(round(x+1),round(y+1),round(x+width-1),round(y+height-1),0);

//limit the length
if(active){
if(maxLength &gt; 0){
    while(string_width(keyboard_string)&gt;maxLength){
        content = string_delete(keyboard_string,string_length(keyboard_string),1);
        keyboard_string = content;
    }
    content = keyboard_string;
}
else if(string_width(keyboard_string)&lt;width-6)
    content=keyboard_string;
}

//drawing the text
if(active)
    draw_set_colour(acol3); //active
else
    draw_set_colour(pcol3); //passive

var vstring = content;
if(content!=""){ //visible text
    while(string_width(vstring)&gt;width-6)
        vstring = string_delete(vstring,1,1);
    draw_text(round(x+3),round(y+1),vstring);
}
else{
    draw_set_alpha(0.3);
    draw_text(round(x+3),round(y+1),caption);
    draw_set_alpha(1);
}

// drawing the cursor
if(active)
{
    draw_set_colour(acol1); //active           
    if(date_get_second(date_current_datetime()) mod 2&gt;0){
       for(var i=0; i&lt;cwidth; i++){
           draw_line(round(x+3+i+string_width(vstring)),round(y+1),round(x+3+i+string_width(vstring)),round(y+14))
       }
   }
}

if(callback &gt;= 0)
    script_execute(callback,id);
</string>
